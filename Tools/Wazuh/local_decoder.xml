<decoder name="yara_decoder">
    <prematch>^YARA scan result:</prematch>
</decoder>

<decoder name="yara_decoder_fields">
    <parent>yara_decoder</parent>
    <regex>^YARA scan result: \s*(\S+)(?: \s*\[(.*?)\])?\s+(\S+)</regex>
    <order>yara.rule, yara.meta, yara.filepath</order>
    <description>Yara rule match detected.</description>
</decoder>
