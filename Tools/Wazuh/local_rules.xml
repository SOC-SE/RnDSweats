<group name="yara,">
  <rule id="100100" level="12">
    <decoded_as>yara</decoded_as>
    <description>Yara scan detected a match.</description>
    <match>^yara</match>
  </rule>
  <rule id="100101" level="12">
    <if_sid>100100</if_sid>
    <field name="yara_rule">\.+</field>
    <description>Yara rule: $(yara_rule) matched on file: $(yara_file)</description>
    <mitre>
      <id>T1027</id>
    </mitre>
  </rule>
</group>
